#deleting elements from a dictionary
mydict={
    "a":"number",
    "b":"no2",
    "c":"num3"
    }
print(mydict["c"])
mydict["python"]="cool"
del mydict["a"]
print(mydict)

#accesing elements from a tuple
mylist=('1','a','3')
print(mylist[2])

#assigning elemts to a list
number1 = [1,2,3,4,5,6,7,8]
number2 = [10,20,30,40,50]

number1.append(99)
print(number1)
